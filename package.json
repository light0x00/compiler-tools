{
	"name": "compiler-tools",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"keywords": [],
	"author": "",
	"license": "MIT",
	"scripts": {
		"lint": "eslint src/**/*.ts test/**/*.ts",
		"lint:fix": "npm run lint -- --fix",
		"type-check": "tsc --noEmit",
		"type-check:watch": "npm run type-check -- --watch",
		"build:types": "tsc --emitDeclarationOnly",
		"//babel": "ü§™babel-cli has too many configurations, so let it stand alone",
		"babel": "babel --extensions \".ts\" --copy-files",
		"build:js": "npm run babel -- src --out-dir dist/src && npm run babel -- test --out-dir dist/test",
		"build": "npm run build:types && npm run build:js",
		"//build:watch": "ü§êfor debug,babel-node does not support debugging",
		"build:watch": "npm run build:js -- --watch",
		"pretest": "npm run build",
		"test": "cross-env DEBUG=APP:* mocha --harmony --require module-alias/register --file dist/test/*.js"
	},
	"_moduleAliases": {
		"@": "dist/src",
		"test": "dist/test"
	},
	"pre-commit": [
		"echo 'pre-commit'"
	],
	"dependencies": {
		"debug": "^4.1.1",
		"lodash": "^4.17.15",
		"module-alias": "^2.2.2",
		"named-regexp-groups": "^1.0.5"
	},
	"devDependencies": {
		"@babel/cli": "^7.7.4",
		"@babel/core": "^7.7.4",
		"@babel/node": "^7.7.4",
		"@babel/plugin-proposal-class-properties": "^7.4.0",
		"@babel/plugin-proposal-numeric-separator": "^7.2.0",
		"@babel/plugin-proposal-object-rest-spread": "^7.4.0",
		"@babel/plugin-proposal-optional-chaining": "^7.7.4",
		"@babel/plugin-transform-named-capturing-groups-regex": "^7.7.4",
		"@babel/preset-env": "^7.7.4",
		"@babel/preset-typescript": "^7.7.4",
		"@babel/register": "^7.7.4",
		"@types/debug": "^4.1.5",
		"@types/lodash": "^4.14.144",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.6",
		"@typescript-eslint/eslint-plugin": "^2.9.0",
		"@typescript-eslint/parser": "^2.9.0",
		"codecov": "^3.6.1",
		"cross-env": "^6.0.3",
		"eslint": "^6.7.1",
		"mocha": "^6.2.2",
		"pre-commit": "^1.2.2",
		"should": "^13.2.3",
		"ts-node": "^8.4.1",
		"tsconfig-paths": "^3.9.0",
		"typescript": "^3.7.2"
	}
}
